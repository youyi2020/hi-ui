{
  "name": "hi-ui",
  "version": "1.0.0",
  "description": "A Component Library for Vue.js.",
  "main": "lib/hi-ui.common.js",
  "files": [
    "lib",
    "src",
    "packages",
    "types"
  ],
  "typings": "types/index.d.ts",
  "scripts": {
    "bootstrap": "yarn || npm i",
    "build:file": "node build/bin/iconInit.js",
    "deploy:build": "cross-env NODE_ENV=production webpack --config build/webpack.demo.js",
    "dev": "npm run bootstrap && cross-env NODE_ENV=development webpack-dev-server --config build/webpack.demo.js & node",
    "dev:play": "cross-env NODE_ENV=development PLAY_ENV=true webpack-dev-server --config build/webpack.demo.js",
    "clean": "rimraf lib && rimraf packages/*/lib && rimraf test/**/coverage"
  },
  "author": "youyi.sun@aispeech.com",
  "keywords": [
    "eleme",
    "vue",
    "components"
  ],
  "license": "MIT",
  "peerDependencies": {
    "vue": "^2.5.16"
  },
  "devDependencies": {
    "autoprefixer": "^9.4.7",
    "babel-core": "^6.22.1",
    "babel-loader": "^7.1.1",
    "babel-preset-env": "^1.3.2",
    "babel-preset-stage-2": "^6.22.0",
    "cheerio": "^1.0.0-rc.2",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.0",
    "file-loader": "^3.0.1",
    "highlight.js": "^9.14.2",
    "html-webpack-plugin": "^3.2.0",
    "markdown-it": "^8.4.2",
    "markdown-it-anchor": "^5.0.2",
    "markdown-it-container": "^2.0.0",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.11.0",
    "postcss": "^7.0.14",
    "postcss-loader": "^3.0.0",
    "progress-bar-webpack-plugin": "^1.12.1",
    "rimraf": "^2.6.3",
    "sass-loader": "^7.1.0",
    "striptags": "^3.1.1",
    "style-loader": "^0.23.1",
    "transliteration": "^2.1.3",
    "url-loader": "^1.1.2",
    "vue-loader": "^15.6.3",
    "vue-markdown-loader": "^2.4.1",
    "vue-router": "^3.0.2",
    "vue-template-compiler": "^2.6.6",
    "webpack": "^4.29.5",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.1.14",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "vue": "^2.5.16"
  }
}
